{
    "name": "evotic/solus-sdk",
    "description": "This is a php sdk for the solusvm v2 api, designed for Laravel",
    "version": "1.0.4",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/evoticde/solus-sdk/issues",
        "source": "https://github.com/evoticde/solus-sdk"
    },
    "autoload": {
        "psr-4": {
            "Evotic\\SolusSDK\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Leon Hausdorf",
            "email": "leon.hausdorf@evotic.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.21|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.1"
    },
    "conflict": {
        "laravel/framework": "<9.37.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Evotic\\SolusSDK\\SolusBaseServiceProvider"
            ]
        }
    }
}
